{
  "name": "通过退格键返回上一页",
  "version": "3.0",
  "manifest_version": 2,
  "description": "通过退格键（Backspace）返回到之前浏览的页面。",
  "homepage_url": "https://github.com/justjavac/back-to-backspace-for-back",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "js/content_scripts.js"
      ]
    }
  ]
}